<div class="col-md-4 mt-4 mx-auto">
	<div class="card">
		<div class="card-header text-dark" id="headerNoteWarning">
			<h3>Crear Nota Warning</h3>
		</div>
		<div class="card-body">
			<form action="/main/newNoteWarning" method="POST">
				<div class="form-group">
					<input type="text" name="title" placeholder="Titulo" class="form-control" autofocus="">
				</div>
				<div class="form-group">
					<textarea type="text" name="description" placeholder="Descripcion" class="form-control"></textarea>
				</div>
				<div class="form-group">
					<button type="submit" class="btn btn-outline-primary  font-weight-bolder">Guardar</button>
				</div>
			</form>
		</div>
	</div>
</div>